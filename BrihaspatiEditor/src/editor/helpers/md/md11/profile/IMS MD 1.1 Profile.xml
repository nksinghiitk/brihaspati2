<?xml version="1.0"?>
<!-- RELOAD Metadata Profile -->
<profile vocabfile="IMS MD 1.1 Vocab.xml" schemahelperfile="IMS MD 1.1 Helper.xml">

  <group name="Meta-metadata">
    <element name="Identifier" path="record/metametadata/identifier"/>
    <group name="Catalogue Entry">
      <element name="Catalogue" path="record/metametadata/catalogentry/catalogue"/>
      <element name="Entry" path="record/metametadata/catalogentry/entry/langstring"/>
    </group>
    <group name="Contribution">
      <element name="Role" path="record/metametadata/contribute/role/langstring"/>
      <element name="VCard" path="record/metametadata/contribute/centity/vcard"/>
      <element name="Date" path="record/metametadata/contribute/date/datetime"/>
    </group>
    <element name="Metadatascheme" path="record/metametadata/metadatascheme"/>
    <element name="Language" path="record/metametadata/language"/>
  </group>

  <group name="General">
    <element name="Identifier" path="record/general/identifier"/>
    <element name="Title" path="record/general/title/langstring"/>
    <group name="Catalogue Entry">
      <element name="Catalogue" path="record/general/catalogentry/catalogue"/>
      <element name="Entry" path="record/general/catalogentry/entry/langstring"/>
    </group>
    <element name="Language" path="record/general/language"/>
    <element name="Description" path="record/general/description/langstring"/>
    <element name="Keywords" path="record/general/keywords/langstring"/>
    <element name="Coverage" path="record/general/coverage/langstring"/>
    <element name="Structure" path="record/general/structure/langstring"/>
    <element name="Aggregation Level" path="record/general/aggregationlevel"/>
  </group>

  <group name="Life Cycle">
    <element name="Version" path="record/lifecycle/version/langstring"/>
    <element name="Status" path="record/lifecycle/status/langstring"/>
    <group name="Contribution">
      <element name="Role" path="record/lifecycle/contribute/role/langstring"/>
      <element name="VCard" path="record/lifecycle/contribute/centity/vcard"/>
      <element name="Date" path="record/lifecycle/contribute/date/datetime"/>
    </group>
  </group>

  <group name="Technical">
    <element name="Format" path="record/technical/format/langstring"/>
    <element name="Size" path="record/technical/size"/>
    <element name="Location" path="record/technical/location"/>
    <group name="Requirements">
      <element name="Type" path="record/technical/requirements/type/langstring"/>
      <element name="Name" path="record/technical/requirements/name/langstring"/>
      <element name="Minimum Version" path="record/technical/requirements/minimumversion"/>
      <element name="Maximum Version" path="record/technical/requirements/maximumversion"/>
    </group>
    <element name="Installation Remarks" path="record/technical/installationremarks/langstring"/>
    <element name="Other Platform Requirements" path="record/technical/otherplatformrequirements/langstring"/>
    <element name="Duration" path="record/technical/duration/datetime"/>
  </group>

  <group name="Educational">
    <element name="Interactivity Type" path="record/educational/interactivitytype/langstring"/>
    <element name="Learning Resource Type" path="record/educational/learningresourcetype/langstring"/>
    <element name="Interactivity Level" path="record/educational/interactivitylevel/langstring"/>
    <element name="Semantic Density" path="record/educational/semanticdensity"/>
    <element name="Intended end user role" path="record/educational/intendedenduserrole/langstring"/>
    <element name="Learning Context" path="record/educational/learningcontext/langstring"/>
    <element name="Typical age range" path="record/educational/typicalagerange/langstring"/>
    <element name="Difficulty" path="record/educational/difficulty"/>
    <element name="Typical learning time" path="record/educational/typicallearningtime/datetime"/>
    <element name="Description" path="record/educational/description/langstring"/>
    <element name="Language" path="record/educational/language"/>
  </group>

  <group name="Rights">
    <element name="Cost" path="record/rights/cost"/>
    <element name="Copyright and other restrictions" path="record/rights/copyrightandotherrestrictions"/>
    <element name="Description" path="record/rights/description/langstring"/>
  </group>

  <group name="Relation">
    <element name="Kind" path="record/relation/kind/langstring"/>
    <group name="Resource">
      <element name="Identifier" path="record/relation/resource/identifier"/>
      <element name="Description" path="record/relation/resource/description/langstring"/>
    </group>
  </group>

  <group name="Annotation">
    <element name="Person" path="record/annotation/centity/vcard"/>
    <element name="Date" path="record/annotation/date/datetime"/>
    <element name="Description" path="record/annotation/description/langstring"/>
  </group>

  <group name="Classification">
    <element name="Purpose" path="record/classification/purpose/langstring"/>
    <group name="Taxon Path">
      <element name="Source" path="record/classification/taxonpath/source"/>
      <group name="Taxon">
        <element name="ID" path="record/classification/taxonpath/taxon/id"/>
        <element name="Entry" path="record/classification/taxonpath/taxon/entry/langstring"/>
      </group>
    </group>
    <element name="Description" path="record/classification/description/langstring"/>
    <element name="Keywords" path="record/classification/keywords/langstring"/>
  </group>

</profile>
